<h1>Publishers</h1>

<table>
  <thead>
  <tr>
    <th>Publisher ID</th>
    <th>Name</th>
    <th>Active</th>
    <th></th>
  </tr>
  </thead>
  <% @publishers.sort!{|a,b| a.id<=> b.id} %>
  <% for publisher in @publishers %>
  <tbody>
  <tr>
    <td><%=publisher.id%></td>
    <td> <%=publisher.name%></td>
    <td><%=publisher.active%></td>
    <td><a href="/publishers/<%=publisher.id%>">Edit</a></td>
  </tr>
  </tbody>
  <% end %>
</table>
<a href="/publishers/new">New Publisher</a>
